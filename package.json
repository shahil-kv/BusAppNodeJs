{
  "name": "@node-nx/source",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build --configuration=production",
    "build:prod": "NODE_ENV=production nx build --configuration=production",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "dev": "NODE_ENV=development npm run prisma:db && npm run prisma:generate && nx serve --configuration=development",
    "dev:worker": "NODE_ENV=development npm run prisma:generate && npm run build && node dist/Node_NX/src/worker.js",
    "dev:app": "NODE_ENV=development npm run prisma:db && npm run prisma:generate && PORT=8080 nx serve --configuration=development",
    "dev:worker-only": "NODE_ENV=development npm run prisma:generate && npm run build && PORT=8081 node dist/Node_NX/src/worker.js",
    "dev:worker-memory": "NODE_ENV=development npm run prisma:generate && npm run build && PORT=8081 node --max-old-space-size=4096 --expose-gc dist/Node_NX/src/worker.js",
    "dev:worker-aggressive": "NODE_ENV=development npm run prisma:generate && npm run build && PORT=8081 node --max-old-space-size=8192 --expose-gc --optimize-for-size dist/Node_NX/src/worker.js",
    "clear-memory": "./scripts/clear-memory.sh",
    "cloud": "NODE_ENV=production npm run prisma:db && npm run prisma:generate && nx serve --configuration=production",
    "prisma:generate": "node scripts/prisma-env.js prisma generate",
    "prisma:migrate": "node scripts/prisma-env.js prisma migrate dev",
    "prisma:studio": "node scripts/prisma-env.js prisma studio",
    "prisma:db": "node scripts/prisma-env.js prisma db pull"
  },
  "nx": {
    "includedScripts": []
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@deepgram/sdk": "^4.5.1",
    "@elevenlabs/elevenlabs-js": "^2.4.0",
    "@google-cloud/speech": "^7.1.0",
    "@google-cloud/text-to-speech": "^6.1.0",
    "@google-cloud/vertexai": "^1.10.0",
    "@google/genai": "^1.0.0",
    "@huggingface/inference": "^4.0.6",
    "@pinecone-database/pinecone": "^6.1.1",
    "@prisma/client": "^6.7.0",
    "@supabase/supabase-js": "^2.50.1",
    "@types/socket.io": "^3.0.1",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.56.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.1",
    "from": "^0.1.7",
    "helmet": "^8.1.0",
    "import": "^0.0.6",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "libphonenumber-js": "^1.12.8",
    "mailgen": "^2.0.29",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "pdf-parse": "^1.1.1",
    "request-ip": "^3.3.0",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.6.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/js": "^9.8.0",
    "@nx/esbuild": "20.4.2",
    "@nx/eslint": "20.4.2",
    "@nx/eslint-plugin": "20.4.2",
    "@nx/jest": "20.4.2",
    "@nx/js": "20.4.2",
    "@nx/node": "20.4.2",
    "@nx/workspace": "20.4.2",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "~18.16.9",
    "esbuild": "^0.19.2",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "lint-staged": "^15.4.3",
    "nx": "20.4.2",
    "prettier": "^2.6.2",
    "prisma": "^6.7.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.3.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.19.0"
  }
}
